{
  "name": "joke-api",
  "version": "1.0.0",
  "description": "An API for dad and chuck norris jokes",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "mocha src/**/*.spec.ts --require ts-node/register --reporter spec",
