    "@nestjs/testing": "^6.3.1",
    "@nestjs/websockets": "^6.3.1",
    "axios": "^0.21.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rxjs": "^6.5.2",
    "woodcutter": "^0.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/sinon": "^7.0.12",
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "sinon": "^7.3.2",
